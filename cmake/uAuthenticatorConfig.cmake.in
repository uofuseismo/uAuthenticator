# Tell the user project where to find our headers and libraries

set (uAuthenticator_VERSION "@PROJECT_VERSION@")

set (uAuthenticator_INCLUDE_DIRS "@CMAKE_INSTALL_PREFIX@/@PROJECT_INSTALL_INCLUDEDIR@")
set (uAuthenticator_LIBRARY_DIRS "@CMAKE_INSTALL_PREFIX@/@PROJECT_INSTALL_LIBDIR@")

# Allows loading uAuthenticator settings from another project
set (uAuthenticator_CONFIG_FILE "${CMAKE_CURRENT_LIST_FILE}")

# List of compilation flags -DTOTO to export
set (uAuthenticator_DEFINITIONS "@uAuthenticator_DEFINITIONS@")

# Our library dependencies (contains definitions for IMPORTED targets)
#include ("${CMAKE_CURRENT_LIST_DIR}/uAuthenticatorLibraryDepends.cmake")

# These are IMPORTED targets created by uAuthenticatorLibraryDepends.cmake
set (uAuthenticator_LIBRARIES "@PROJECT_LIBRARIES@")
